{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30,
      "includeFiles": "models/**",
      "excludeFiles": "**/*.pyc,__pycache__/**,*.pyo,*.pyd,.git/**,*.md,*.txt,!requirements.txt,!runtime.txt"
    }
  }
}
