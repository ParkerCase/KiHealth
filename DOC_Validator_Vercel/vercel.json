{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30
    }
  }
}
